<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.example.nikolay.calculator.ResultatActivity"
    android:background="@drawable/nebo2"
    android:clickable="true">

    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:id="@+id/linearLayout"
        android:alpha="0.7">

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:text="Выбирите первую КПП"
            android:id="@+id/vibor_2_kpp"
            android:textAlignment="center"
            android:layout_weight="1"
            android:textColor="#ffffff" />
    </LinearLayout>

    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:layout_centerHorizontal="true"
        android:layout_alignParentStart="false"
        android:layout_alignParentEnd="false"
        android:id="@+id/linearLayout2"
        android:layout_below="@+id/linearLayout"
        android:alpha="0.7">

        <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/k111"
            android:layout_weight="1"
            android:textColor="#ffffff"
            android:textAlignment="center"
            android:inputType="numberDecimal"
            android:textSize="12dp" />

        <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/k112"
            android:layout_weight="1"
            android:textColor="#ffffff"
            android:textAlignment="center"
            android:inputType="numberDecimal"
            android:textSize="12dp" />

        <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/k113"
            android:layout_weight="1"
            android:textColor="#ffffff"
            android:textAlignment="center"
            android:inputType="numberDecimal"
            android:textSize="12dp" />

        <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/k114"
            android:layout_weight="1"
            android:textColor="#ffffff"
            android:textAlignment="center"
            android:inputType="numberDecimal"
            android:textSize="12dp" />

        <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/k1_z"
            android:layout_weight="1"

            android:textColor="#ffffff"
            android:textAlignment="center"
            android:inputType="numberDecimal"
            android:textSize="12dp" />
    </LinearLayout>

    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:id="@+id/linearLayout4"
        android:layout_below="@+id/linearLayout2">

        <Button
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:text="газ 51"
            android:layout_weight="1"
            android:id="@+id/button5"
            android:onClick="onClick_gaz_51"
            android:alpha="0.5" />

        <Button
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:text="ваз 2108"
            android:layout_weight="1"
            android:id="@+id/button6"
            android:alpha="0.5"
            android:onClick="onClick_vaz2108" />
    </LinearLayout>

    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/linearLayout4"
        android:layout_alignParentStart="true"
        android:id="@+id/linearLayout5">

        <Button
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:text="газ 52"
            android:layout_weight="1"
            android:id="@+id/button7"
            android:onClick="onClick_gaz_52_3"
            android:alpha="0.5" />

        <Button
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:text="ваз ока"
            android:layout_weight="1"
            android:id="@+id/button8"
            android:alpha="0.5"
            android:onClick="onClick_vaz_oka" />
    </LinearLayout>

    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:layout_below="@+id/linearLayout5"
        android:id="@+id/linearLayout6">

        <Button
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:text="волга 2410"
            android:layout_weight="1"
            android:id="@+id/button11"
            android:alpha="0.5"
            android:onClick="onClick_volga" />

        <Button
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:text="ваз 2101"
            android:layout_weight="1"
            android:id="@+id/button14"
            android:onClick="onClick_vaz2101"
            android:alpha="0.5" />
    </LinearLayout>

    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:layout_below="@+id/linearLayout6"
        android:id="@+id/linearLayout7">

        <Button
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:text="мото &quot;урал&quot;"
            android:layout_weight="1"
            android:id="@+id/button15"
            android:alpha="0.5"
            android:onClick="onClick_ural" />

        <Button
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:text="рк уаз"
            android:layout_weight="1"
            android:id="@+id/button16"
            android:alpha="0.5"
            android:onClick="onClick_uaz_rk" />
    </LinearLayout>

    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:layout_below="@+id/linearLayout7"
        android:id="@+id/linearLayout8">

        <Button
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:text="кпп уаз"
            android:layout_weight="1"
            android:id="@+id/button"
            android:alpha="0.5"
            android:onClick="onClick_uaz" />

        <Button
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:text="рк нива"
            android:layout_weight="1"
            android:id="@+id/button2"
            android:alpha="0.5"
            android:onClick="onClick_niva_rk" />
    </LinearLayout>

    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:layout_below="@+id/linearLayout8"
        android:id="@+id/linearLayout9">

        <Button
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:text="м412"
            android:id="@+id/button18"
            android:layout_weight="1"
            android:onClick="onClick10"
            android:alpha="0.5" />

        <Button
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:text="газ 69"
            android:id="@+id/button19"
            android:layout_weight="1"
            android:alpha="0.5"
            android:onClick="onClick_gaz69" />
    </LinearLayout>

    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:layout_alignParentEnd="false"
        android:layout_below="@+id/linearLayout9"
        android:layout_alignParentStart="true"
        android:baselineAligned="true"
        android:id="@+id/linearLayout10">

        <Button
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:text="дальше"
            android:id="@+id/button21"
            android:layout_weight="1"
            android:onClick="sendData"
            android:textAlignment="center"
            android:allowUndo="true"
            android:alpha="0.5" />
    </LinearLayout>

    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_alignTop="@+id/linearLayout2"
        android:layout_toEndOf="@+id/linearLayout2"
        android:layout_marginStart="71dp"></FrameLayout>

</RelativeLayout>
